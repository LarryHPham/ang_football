    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "webfontloader": "^1.6.26",
    "webpack-uglify-js-plugin": "^1.1.9",
    "zone.js": "~0.6.26"
  },
  "devDependencies": {
